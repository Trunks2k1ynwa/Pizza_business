{
  "name": "naturebeauty",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:js": "prettier --write \"src/**/*.js\"",
    "format:json": "prettier --write \"src/**/*.json\"",
    "format:ts": "prettier --write \"src/**/*.ts{,x}\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:check": "prettier --check .",
    "format:check:js": "prettier --check \"src/**/*.js\"",
    "format:check:json": "prettier --check \"src/**/*.json\"",
    "format:check:ts": "prettier --check \"src/**/*.ts{,x}\"",
    "format:check:md": "prettier --check \"**/*.md\""
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run format",
      "yarn run lint",
      "git add ."
    ],
    "*.{json}": [
      "yarn run format",
      "git add ."
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/react-query-devtools": "^4.29.12",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.3.6",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "firebase": "^9.21.0",
    "html-react-parser": "^3.0.16",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "npm": "^9.6.6",
    "quill-image-uploader": "^1.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-login": "^5.2.2",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.0",
    "react-loading-skeleton": "^3.3.1",
    "react-query-devtools": "^2.6.3",
    "react-quill": "^2.0.0-beta.4",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.3",
    "slick-carousel": "^1.8.1",
    "slugify": "^1.6.6",
    "styled-components": "^5.3.10",
    "sweetalert2": "^11.7.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.23",
    "sass": "^1.62.0",
    "tailwindcss": "^3.3.1",
    "vite": "^4.3.0",
    "vite-plugin-alias": "^0.0.7"
  }
}
